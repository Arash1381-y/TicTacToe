<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="/userInterface/styleSheets/GameSetUpStyleSheet.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.controllers.GameSetUpController">
   <children>
      <VBox fx:id="chart" alignment="TOP_CENTER" layoutX="159.0" layoutY="74.0" prefHeight="251.0" prefWidth="281.0">
         <children>
            <Label alignment="TOP_CENTER" contentDisplay="CENTER" text="Game SetUp" />
            <Label prefHeight="18.0" prefWidth="214.0" />
            <Label />
            <Label prefHeight="18.0" prefWidth="106.0" text="Opponent name : " />
            <TextField fx:id="opponentName" onMouseClicked="#removeDemo" prefHeight="26.0" prefWidth="0.0" text="exp : jac..." />
            <Label fx:id="NoLoginPlayer" />
            <Label fx:id="opponentNotFound" />
            <Button mnemonicParsing="false" onMouseClicked="#switchToGame" prefHeight="26.0" prefWidth="111.0" styleClass="option" text="Start" />
         </children>
      </VBox>
      <Button layoutX="545.0" layoutY="14.0" mnemonicParsing="false" onMouseClicked="#switchToWelcomeScene" styleClass="option" text="Back" />
   </children>
</AnchorPane>
